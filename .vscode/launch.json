{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Main Process",
            "cwd": "${workspaceRoot}",
            "envFile": "${workspaceFolder}/.vscode/launch.env",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "start"
            ],
            "outputCapture": "std",
            "port": 5858
        },{
            "name": "Debug Rust (Javascript)",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "args": [ "${workspaceFolder}/rust-tester/index.js" ],
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "debug-rust"
            ],
            "port": 9229
        }, {
            "name": "Debug Rust (Linux)",
            "type": "lldb",
            "request": "launch",
            "program": "/usr/bin/node",
            "args": [ "${workspaceFolder}/rust-test/index.js" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "terminal.integrated.automationShell.linux": "bash",
            "preLaunchTask": "build",
            "externalConsole": false,
        },                  
    ]
}