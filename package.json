{
  "name": "commander",
  "version": "1.0.0",
  "description": "A Norton Commander clone based on Electron and  Web Components",
  "main": "electron/main.js",
  "scripts": {
    "builddev": "webpack --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "start": "npm run builddev && electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "de.uriegel.commander",
    "copyright": "Copyright 2021 Uwe Riegel",
    "productName": "Commander",
    "electronVersion": "15.2.0",
    "nodeGypRebuild": false,
    "linux": {
      "target": [
        "AppImage",
        "rpm"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uriegel/window-commander.git"
  },
  "keywords": [
    "electron",
    "javascript",
    "nodejs",
    "web",
    "components"
  ],
  "author": "Uwe Riegel <uriegel@hotmail.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uriegel/window-commander/issues"
  },
  "homepage": "https://github.com/uriegel/window-commander#readme",
  "dependencies": {
    "electron-settings": "^4.0.2",
    "filesystem-utilities": "5.0.1",
    "shared-module": "file:shared-module",
    "theme-change-detect": "0.0.1"
  },
  "devDependencies": {
    "electron": "^15.2.0",
    "electron-builder": "^22.13.1",
    "grid-splitter": "^0.1.0",
    "virtual-table-component": "^1.1.1",
    "web-dialog-box": "^2.0.2",
    "web-electron-titlebar": "^0.2.0",
    "web-menu-bar": "^0.3.0",
    "web-pie-progress": "^0.0.1",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
