{
  "name": "commander",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "cp": "^0.2.0",
    "electron": "^23.0.0",
    "menubar-react": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "virtual-table-react": "^8.2.2",
    "web-dialog-react": "^1.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "BUILD_PATH='./resources/web' GENERATE_SOURCEMAP='false' react-scripts build && npm run copy",
    "electron": "tsc -p electron/typescript",
    "copy": "cp resources/web/static/js/*.js resources/script.js && cp resources/web/static/css/*.css resources/style.css"
  },
  "eslintConfig": {
    "rules": {
      "eqeqeq": "off",
      "no-mixed-operators": "off"
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
