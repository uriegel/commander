{
  "name": "commander",
  "version": "1.0.0",
  "description": "A Norton Commander clone based on Electron, F# and  Web Components",
  "main": "electron/dist/main.js",
  "scripts": {
    "tsc": "tsc -p electron/typescript",
    "old-builddev": "webpack --config webpack.dev.js",
    "old-build": "npm run build-rust-release && webpack --config webpack.prod.js",
    "old-start": "npm run tsc && npm run build-rust-release && npm run builddev && electron .",
    "old-dist": "npm run build && electron-builder",
    "old-debug-rust": "npm run build-rust-debug && node --nolazy --inspect-brk=9229 rust-test/index.js",
    "build-rust": "cargo-cp-artifact -nc local-modules/rust-addon/index.node -- cargo build --message-format=json-render-diagnostics",
    "old-build-rust-debug": "npm run build-rust --",
    "old-build-rust-release": "npm run build-rust -- --release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uriegel/window-commander.git"
  },
  "keywords": [
    "electron",
    "javascript",
    "nodejs",
    "web",
    "components"
  ],
  "author": "Uwe Riegel <uriegel@hotmail.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uriegel/window-commander/issues"
  },
  "homepage": "https://github.com/uriegel/window-commander#readme",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "electron": "^16.0.5",
    "grid-splitter": "^1.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "virtual-table-component": "^3.0.0",
    "web-dialog-box": "^3.0.1",
    "web-electron-titlebar": "^1.0.0",
    "web-menu-bar": "^1.0.1",
    "web-pie-progress": "^1.0.0",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1",
    "webpack-merge": "^5.8.0"
  }
}
